<workbook>
	<worksheet name="Sheet1">
		<section startRow="0" endRow="0" />
		<loop startRow="1" endRow="1" items="result" var="RBTParameters"
			varType="com.onmobile.apps.ringbacktones.genericcache.beans.Parameters">
			<section startRow="1" endRow="1">
				<mapping row="1" col="0">RBTParameters.type</mapping><!-- Handle this -->
				<mapping row="1" col="1">RBTParameters.param</mapping>
				<mapping row="1" col="2">RBTParameters.value</mapping>
				<mapping row="1" col="3">RBTParameters.info</mapping>
			</section>
			<loopbreakcondition>
				<rowcheck offset="0">
					<cellcheck offset="0" />
				</rowcheck>
			</loopbreakcondition>
		</loop>
	</worksheet>
</workbook>