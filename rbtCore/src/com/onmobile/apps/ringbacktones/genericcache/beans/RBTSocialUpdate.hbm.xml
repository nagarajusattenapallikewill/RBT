<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.onmobile.apps.ringbacktones.genericcache.beans.RBTSocialUpdate" table="rbt_social_update">
		
			<id name="id" column="SEQUENCE_ID" unsaved-value="0" type="java.lang.Long"><generator class="increment"/></id>
		

		<property column="MSISDN" length="50" name="msisdn" type="java.lang.String" />
		<property column="CALLER_ID" length="50" name="callerId" type="java.lang.String" />
		<property column="CLIP_ID"  name="clipId" type="com.onmobile.apps.ringbacktones.genericcache.datatype.GCInteger" />
		<property column="CAT_ID"  name="catId" type="com.onmobile.apps.ringbacktones.genericcache.datatype.GCInteger" />
		<property column="EVENT_TYPE"  name="eventType" type="java.lang.Long" />
		<property column="START_TIME" name="startTime" type="java.sql.Timestamp" />
		<property column="END_TIME" name="endTime" type="java.sql.Timestamp" />
		<property column="STATUS"  name="status" type="com.onmobile.apps.ringbacktones.genericcache.datatype.GCInteger" />
		<property column="RBT_TYPE" length="20" name="RBTType" type="java.lang.String" />
		
	</class>
</hibernate-mapping>
