<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.onmobile.apps.ringbacktones.genericcache.beans.ChargeClassMap" table="RBT_CHARGE_CLASS_MAP">
		<id name="chargeClass" type="com.onmobile.apps.ringbacktones.genericcache.datatype.GCString">
			<column name="CLASS_TYPE" not-null="true" />
