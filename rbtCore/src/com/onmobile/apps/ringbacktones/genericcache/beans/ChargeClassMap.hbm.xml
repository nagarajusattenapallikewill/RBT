		<property column="REGEX_SMS_OR_VOICE" length="64" name="regexSmsorVoice" type="java.lang.String" />
		<property column="ACCESS_MODE" length="32" name="accessMode" type="java.lang.String" />
		<property column="FINAL_CLASS_TYPE" length="64" name="finalClasstype" type="java.lang.String" />
	</class>
</hibernate-mapping>