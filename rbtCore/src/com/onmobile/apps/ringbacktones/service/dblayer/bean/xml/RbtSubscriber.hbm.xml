		<property column="PREPAID_YES" name="prepaidYes" type="string" />
		<property column="LAST_ACCESS_DATE" name="lastAccessDate" type="timestamp" />
		<property column="NEXT_CHARGING_DATE" name="nextChargingDate" type="timestamp" />
		<property column="NUM_VOICE_ACCESS" name="numVoiceAccess" type="integer" />
		<property column="ACTIVATION_INFO" name="activationInfo" type="string" />
