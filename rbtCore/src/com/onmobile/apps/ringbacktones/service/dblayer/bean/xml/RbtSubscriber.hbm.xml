		<id name="subscriberId" type="string" column="SUBSCRIBER_ID" />
		</id>
       	<property column="ACTIVATED_BY" name="activatedBy" type="string" />
		<property column="DEACTIVATED_BY" name="deactivatedBy" type="string" />
		<property column="START_DATE" name="startDate" type="timestamp" />
