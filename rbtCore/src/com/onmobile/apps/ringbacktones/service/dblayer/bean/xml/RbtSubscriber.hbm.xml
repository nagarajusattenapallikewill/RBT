		<property column="GENDER" name="gender" type="character" />
		<property column="EXTRA_INFO" name="extraInfo" type="string" />
		<property column="CIRCLE_ID" name="circleId" type="string" />
		<property column="INTERNAL_REF_ID" name="refId" type="string" />
	</class>
</hibernate-mapping>