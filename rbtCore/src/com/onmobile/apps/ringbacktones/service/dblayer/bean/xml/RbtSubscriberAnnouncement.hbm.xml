<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.onmobile.apps.ringbacktones.service.dblayer.bean.RbtSubscriberAnnouncement" table="RBT_SUBSCRIBER_ANNOUNCEMENTS">
		
		<id name="sequenceId" type="long" column="SEQUENCE_ID" />
			<generator class="increment"/>
		</id>
       	<property column="SUBSCRIBER_ID" name="subscriberId" type="string" />
		<property column="CLIP_ID" name="clipId" type="integer" />
		<property column="STATUS" name="status" type="integer" />
		<property column="START_TIME" name="activationDate" type="timestamp" />
		<property column="END_TIME" name="deactivationDate" type="timestamp" />
		<property column="TIME_INTERVAL" name="timeInterval" type="string" />
		<property column="FREQUENCY" name="frequency" type="string" />
	</class>
</hibernate-mapping>