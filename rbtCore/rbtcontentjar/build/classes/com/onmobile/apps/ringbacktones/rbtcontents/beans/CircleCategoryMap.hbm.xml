<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.onmobile.apps.ringbacktones.rbtcontents.beans.CircleCategoryMap" select-before-update="true" table="RBT_CATEGORY_CIRCLE_MAP">
		<composite-id>
			<key-property column="CIRCLE_ID" name="circleId" type="java.lang.String" />
			<key-property column="PARENT_CATEGORY_ID" name="parentCategoryId" type="int" />
			<key-property column="CATEGORY_ID" name="categoryId" type="int" />
			<key-property column="PREPAID_YES" name="prepaidYes" type="char" />
			<key-property column="CATEGORY_LANGUAGE" name="categoryLanguage" type="java.lang.String" />
		</composite-id>
		<property column="CATEGORY_INDEX" name="categoryIndex" type="int" />
	</class>
</hibernate-mapping>
