<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.onmobile.apps.ringbacktones.rbtcontents.beans.PromoMaster" select-before-update="true" table="RBT_PROMO_MASTER">
		<composite-id>
			<key-property column="PROMO_TYPE" length="10" name="promoType" type="java.lang.String" />
			<key-property column="PROMO_CODE" length="10" name="promoCode" type="java.lang.String" />
		</composite-id>
		<property column="CLIP_ID" name="clipId" type="int" />
	</class>
</hibernate-mapping>
